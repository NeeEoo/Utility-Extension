{
    "manifest_version": 2,
    "name": "Utility Extension",
    "version": "1.0",
    "description": "A Utility Extension That help you",
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "icons": {
        "16": "img/icon_16x16.png",
		"32": "img/icon_32x32.png",
        "48": "img/icon_48x48.png",
		"64": "img/icon_64x64.png",
        "128": "img/icon_128x128.png",
        "512": "img/icon_512x512.png",
		"1024": "img/icon_1024x1024.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19x19.png",
            "38": "img/icon_38x38.png"
        },
        "default_title": "Utility",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
		"contextMenus",
        "https://ajax.googleapis.com/"
    ]
}